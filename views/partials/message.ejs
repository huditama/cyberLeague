<% if ( success_msg != "") { %>
  <div class="alert alert-sucess alert-dismissible fade show" role="alert">
      <%= success_msg[0] %>
    <button type="button" class="close" data-dismiss="alert" aria-label="Close">
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
<% } %>


<% if(typeof error_msg != 'undefined') {  %>
  <h1><%= error.msg %></h1>
<% } %>
